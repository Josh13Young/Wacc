# Output:
# true
# true
# true
# false
# false
#

# Program:

begin

  bool isEven(int a) is
    int b = a % 2 ;
    if b == 0
    then
      return true
    else
      return false
    fi
  end

  int a = 8 ;
  int b = 0 ;
  int c = -2 ;
  int d = 3 ;
  int e = -3 ;

  bool ret = call isEven(a) ;
  println ret ;
  bool ret2 = call isEven(b) ;
  println ret2 ;
  bool ret3 = call isEven(c) ;
  println ret3 ;
  bool ret4 = call isEven(d) ;
  println ret4 ;
  bool ret5 = call isEven(e) ;
  println ret5

end