# Output:
# true
# true
# true
# false
# false
#

# Program:

begin

  bool isEven(int a) is
    int b = call mod(a, 2) ;
    if b == 0
    then
      return true
    else
      return false
    fi
  end

  int mod(int a, int b) is
    int c = call abs(a) ;
    int d = call abs(b) ;
    if c < d
    then 
      if b > 0
      then 
        int ret = c;
        return ret
      else
        int ret = c * -1 ;
        return ret
      fi
    else
      c = c - d ;
      int ret = call mod(c, b) ;
      return ret
    fi
  end

  int abs(int a) is
    if a >= 0
    then
      return a
    else
      return -a
    fi
  end

  int a = 8 ;
  int b = 0 ;
  int c = -2 ;
  int d = 3 ;
  int e = -3 ;

  bool ret = call isEven(a) ;
  println ret ;
  bool ret2 = call isEven(b) ;
  println ret2 ;
  bool ret3 = call isEven(c) ;
  println ret3 ;
  bool ret4 = call isEven(d) ;
  println ret4 ;
  bool ret5 = call isEven(e) ;
  println ret5

end