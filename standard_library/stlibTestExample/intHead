# Output:
# 0
# 1
# -2
#

# Program:

begin

  int intHead(int[] arr) is
    if (len(arr) == 0)
    then
      return 0
    else
      int x = arr[0] ;
      return x
    fi
  end

  int[] arr1 = [] ;
  int[] arr2 = [1, 2, 3] ;
  int[] arr3 = [-2, -2] ;
  int ret1 = call intHead(arr1) ;
  println ret1 ;
  int ret2 = call intHead(arr2) ;
  println ret2 ;
  int ret3 = call intHead(arr3) ;
  println ret3

end