# Output:
# false
# false
# false
# true
# true
#

# Program:

begin

  bool isOdd(int a) is
    int b = a % 2 ;
    if b == 0
    then
      return false
    else
      return true
    fi
  end

  int a = 8 ;
  int b = 0 ;
  int c = -2 ;
  int d = 3 ;
  int e = -3 ;

  bool ret = call isOdd(a) ;
  println ret ;
  bool ret2 = call isOdd(b) ;
  println ret2 ;
  bool ret3 = call isOdd(c) ;
  println ret3 ;
  bool ret4 = call isOdd(d) ;
  println ret4 ;
  bool ret5 = call isOdd(e) ;
  println ret5

end